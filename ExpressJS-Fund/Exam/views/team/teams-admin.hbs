<h1>Teams</h1>
<h3>Manage Users</h3>
<div class="form">
	<form action="/team/all" method="POST">
		<label for="user">User</label>
		<select id="user" name="userId">
			{{#each users}}
				<option value="{{_id}}">{{username}}</option>
			{{/each}}
		</select>
		<label for="team">Team</label>
		<select id="team" name="teamId">
			{{#each teams}}
				<option value="{{_id}}">{{name}}</option>
			{{/each}}
		</select>
		<input type="submit" value="Distribute">
	</form>
</div>

</html>